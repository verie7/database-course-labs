postgres=# CREATE TABLE students (student_id SERIAL PRIMARY KEY, name VARCHAR(50), email VARCHAR(50), faculty VARCHAR(20));
CREATE TABLE
postgres=# INSERT INTO students (name, email, faculty) VALUES ('Alymbek', 'alymbek@example.com', 'AMI');
INSERT 0 1
